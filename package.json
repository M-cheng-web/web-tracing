{
  "name": "@web-tracing/monorepo",
  "version": "2.0.8",
  "description": "基于 JS 跨平台插件，为前端项目提供【 行为、性能、异常、请求、资源、路由、曝光、录屏 】监控手段",
  "private": true,
  "packageManager": "pnpm@9.0.6",
  "scripts": {
    "docs": "pnpm run -C docs docs",
    "docs:build": "pnpm run -C docs docs:build",
    "docs:publish": "sh scripts/publish-docs.sh",
    "build": "pnpm run update && esno scripts/build.ts",
    "build:rollup": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c",
    "build:rollup-watch": "cross-env NODE_OPTIONS=\"--max-old-space-size=6144\" rollup -c -w",
    "build:types": "tsc --emitDeclarationOnly && esno scripts/fix-types.ts",
    "update": "esno scripts/update.ts",
    "update:full": "pnpm run update && pnpm run build:types",
    "clean": "rimraf --glob dist types \"packages/*/dist\"",
    "release": "bumpp --execute=\"npm run release:prepare\" --no-push --no-commit --no-tag",
    "release:prepare": "npm run build:types && npm run update",
    "publish": "esno scripts/publish.ts",
    "watch": "esno scripts/build.ts --watch",
    "test:install": "sh scripts/test-install.sh",
    "test:js": "pnpm run -C examples/vanilla dev",
    "test:vue2": "pnpm run -C examples/vue2 start",
    "test:vue3": "pnpm run -C examples/vue3 start",
    "example:publish": "esno scripts/publish-examples.ts",
    "example:publish-js": "sh scripts/examples/js.sh",
    "example:publish-vue2": "sh scripts/examples/vue2.sh",
    "example:publish-vue3": "sh scripts/examples/vue3.sh",
    "test": "vitest",
    "test-coverage": "vitest --coverage"
  },
  "keywords": [
    "埋点",
    "性能",
    "异常",
    "性能采集",
    "异常采集",
    "前端埋点",
    "前端性能采集"
  ],
  "author": "M-cheng-web <2604856589@qq.com>",
  "license": "MIT",
  "devDependencies": {
    "@algolia/client-search": "^4.16.0",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.15.10",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "bumpp": "^9.0.0",
    "consola": "^2.15.3",
    "cross-env": "^7.0.3",
    "esbuild": "0.17.14",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "esno": "^0.17.0",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "jsdom": "^24.1.0",
    "ohmyfetch": "^0.4.21",
    "prettier": "^2.8.7",
    "puppeteer": "^22.11.1",
    "rimraf": "^4.4.1",
    "rollup": "^3.26.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.2",
    "vitepress": "1.0.0-beta.5",
    "vitest": "^1.6.0"
  }
}
